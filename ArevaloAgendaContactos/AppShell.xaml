<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="ArevaloAgendaContactos.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:ArevaloAgendaContactos"
    Shell.FlyoutBehavior="Flyout"
    Title="ArevaloAgendaContactos">

    <TabBar>
        <ShellContent Title="Nuevo" ContentTemplate="{DataTemplate local:AddContactPage}"/>
        <ShellContent Title="Contactos" ContentTemplate="{DataTemplate local:ContactsListPage}"/>
        <ShellContent Title="Logs" ContentTemplate="{DataTemplate local:LogsPage}"/>
    </TabBar>
</Shell>
